<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="action_report_picking_custom" model="ir.actions.report">
            <field name="name">Guía de remisión electrónica</field>
            <field name="model">stock.picking</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">l10n_pe_lft_report.report_picking_custom</field>
            <field name="report_file">l10n_pe_lft_report.report_picking_custom</field>
            <field name="print_report_name">'Guía de remisión - %s - %s' % (object.partner_id.name or '', object.name)
            </field>
            <field name="binding_model_id" ref="stock.model_stock_picking"/>
            <field name="binding_type">report</field>
            <field name="paperformat_id" ref="l10n_pe_lft_report.paperformat_custom"/>
        </record>

