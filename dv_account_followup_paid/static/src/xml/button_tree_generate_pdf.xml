<?xml version="1.0" encoding="utf-8"?>
<templates>
   <t t-name="button_pdf_tree.ListView.Buttons" t-inherit="web.ListView.Buttons">
       <xpath expr="//div[hasclass('o_list_buttons')]" position="after">
           <button type="button" class="btn btn-primary" style="margin-right: 10px;" t-on-click="OnClickOpenPDF">
               PDF
           </button>
           <button type="button" class="btn btn-primary" style="margin-right: 10px;" t-on-click="OnClickOpenXLSX">
               XLSX
           </button>
       </xpath>
   </t>
</templates>
